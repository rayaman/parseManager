ENTRY MAIN
USING extendedDefine
VERSION 4.1 //The version that nested functions was introduced
[MAIN]{
	// New feature functions inside of functions
	a = hmm(5,1)
	"a = $a$"
	b = hmm(hmm(6,hmm(1,1)),hmm(5,3))
	"b = $b$"
}
[hmm:function(a,b)]{
	return a+b
}
